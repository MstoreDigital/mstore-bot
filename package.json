{
  "name": "mstore-bot",
  "version": "1.0.0",
  "main": "build/src/index.js",
  "scripts": {
    "dev": "tsoa spec-and-routes && ts-node -r tsconfig-paths/register src/index.ts",
    "build": "tsoa spec-and-routes && tsc",
    "start": "node build/src/index.js",
    "test": "tsoa spec-and-routes && jest --coverage",
    "test:watch": "tsoa spec-and-routes && jest --watch",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "discord.js": "^14.14.1",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "swagger-ui-express": "^5.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsoa": "^6.0.1",
    "tsyringe": "^4.8.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.10",
    "@types/swagger-ui-express": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "reflect-metadata": "^0.2.1",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
